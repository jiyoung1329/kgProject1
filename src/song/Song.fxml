<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="song.SongController">
   <children>
      <ImageView fx:id="songDefault" fitHeight="600.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/Default%20Screen.png" />
         </image></ImageView>
      <MediaView fx:id="songMedia" fitHeight="600.0" fitWidth="1000.0" />
      <ImageView fitHeight="57.0" fitWidth="156.0" layoutX="14.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/Remain%20Bar.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="65.0" fitWidth="624.0" layoutX="174.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/Reserve.png" />
         </image>
      </ImageView>
      <Button fx:id="songCancelButton" layoutX="805.0" layoutY="28.0" mnemonicParsing="false" onAction="#songCancelProc" prefHeight="50.0" prefWidth="80.0" style="-fx-background-radius: 30; -fx-background-color: #FFB84D;" text="취소" textFill="WHITE">
         <font>
            <Font name="NanumBarunGothicOTF Bold" size="20.0" />
         </font></Button>
      <Button fx:id="songPlayButton" layoutX="898.0" layoutY="28.0" mnemonicParsing="false" onAction="#songStartProc" prefHeight="50.0" prefWidth="80.0" style="-fx-background-radius: 30; -fx-background-color: #83CE8F;" text="시작" textFill="WHITE">
         <font>
            <Font name="NanumBarunGothicOTF Bold" size="20.0" />
         </font></Button>
      <Button fx:id="songSearchButton" layoutX="744.0" layoutY="35.0" mnemonicParsing="false" onAction="#songSearch" opacity="0.0" prefHeight="40.0" prefWidth="46.0" style="-fx-background-radius: 50; -fx-background-color: #FFFFFF;" />
      <Label fx:id="remainSong" layoutX="107.0" layoutY="42.0">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" layoutX="264.0" layoutY="35.0" prefHeight="40.0" prefWidth="472.0" spacing="10.0">
         <children>
            <Label fx:id="num1">
               <font>
                  <Font size="20.0" />
